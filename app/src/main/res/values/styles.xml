<resources>

    <!-- Base -->
    <style name="BaseTheme.Dark" parent="Theme.AppCompat">
        <item name="colorPrimary">@color/material_grey_900</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColorSecondary">#ffffff</item>
        <item name="colorAccent">#ffffff</item>
        <item name="android:divider">#80bebebe</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="BaseTheme.Light" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/material_grey_100</item>
        <item name="android:textColor">#1b1b1b</item>
        <item name="android:textColorPrimary">#1b1b1b</item>
        <item name="android:textColorSecondary">#1b1b1b</item>
        <item name="colorAccent">#4b4b4b</item>
        <item name="android:divider">#80323232</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="BaseTheme.Dialog.Dark" parent="Theme.AppCompat.Dialog">
        <item name="android:textColorPrimary">#ffffff</item>
        <item name="android:textColorSecondary">#c1c1c1</item>
        <item name="android:textColorTertiary">#ffffff</item>   <!-- dialog title -->
        <item name="android:divider">#5f5f5f</item>
        <item name="colorAccent">#c1c1c1</item> <!-- controls -->
        <item name="android:background">@color/material_grey_800</item>
        <item name="android:textColorHint">#aaaaaa</item>
    </style>
    <style name="BaseTheme.Dialog.Light" parent="Theme.AppCompat.Dialog">
        <item name="android:textColorPrimary">#1b1b1b</item>
        <item name="android:textColorSecondary">#4b4b4b</item>
        <item name="android:textColorTertiary">#000000</item>   <!-- dialog title -->
        <item name="android:divider">#d5d5d5</item>
        <item name="colorAccent">#4b4b4b</item> <!-- controls -->
        <item name="android:background">@color/material_grey_50</item>
        <item name="android:textColorHint">#aaaaaa</item>
    </style>
